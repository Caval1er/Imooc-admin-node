{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "none",
        "endOfLine": "auto"
      }
    ],
    "prefer-template": "off",
    "no-path-concat": "off"
  }
}
